@model List<Moonwalkers.Models.InventorySupplier>

<h1>All Inventory Suppliers</h1>

@if (Model.Count == 0)
{
    <p>No Inventory Suppliers yet!</p>
}
else
{
    <table class="table">
        <tr>
            <th>Id
            <th>Supplier Name</th>
        </tr>
        @foreach (InventorySupplier supplier in Model)
        {
            <tr>
                <td>@supplier.Id</td>
                <td>@supplier.Supplier</td>
            </tr>
        }
    </table>
}
